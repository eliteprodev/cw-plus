{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Cw4HandleMsg",
  "anyOf": [
    {
      "description": "Change the admin",
      "type": "object",
      "required": [
        "update_admin"
      ],
      "properties": {
        "update_admin": {
          "type": "object",
          "properties": {
            "admin": {
              "anyOf": [
                {
                  "$ref": "#/definitions/HumanAddr"
                },
                {
                  "type": "null"
                }
              ]
            }
          }
        }
      }
    },
    {
      "description": "Add a new hook to be informed of all membership changes. Must be called by Admin",
      "type": "object",
      "required": [
        "add_hook"
      ],
      "properties": {
        "add_hook": {
          "type": "object",
          "required": [
            "addr"
          ],
          "properties": {
            "addr": {
              "$ref": "#/definitions/HumanAddr"
            }
          }
        }
      }
    },
    {
      "description": "Remove a hook. Must be called by Admin",
      "type": "object",
      "required": [
        "remove_hook"
      ],
      "properties": {
        "remove_hook": {
          "type": "object",
          "required": [
            "addr"
          ],
          "properties": {
            "addr": {
              "$ref": "#/definitions/HumanAddr"
            }
          }
        }
      }
    }
  ],
  "definitions": {
    "HumanAddr": {
      "type": "string"
    }
  }
}
